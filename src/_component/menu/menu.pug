.menu-wrap
  input.toggler(type="checkbox" id="toggler")
  i.icon__menu
  .menu 
    ul
      each item in data.burger.item 
        li
          a(style="padding-right: 20px" href=`${item.link}`).links= item.title
      label(for="toggler") 
        .close
          i(class="icon__cross").x 
          a.text Close Menu
          


