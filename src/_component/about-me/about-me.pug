//- pre=JSON.stringify(data['about-me'], null, 2)
- var sectionTitle = data['about-me'].sectionTitle;
include ../section-title/section-title

.column(span="row")
    h1.about-me__first-title=data['about-me'].firstTitle 
.column(span="row")
    h2.about-me__second-title=data['about-me'].secondTitle
.column(span="row" height="10")
.column(span="row")
    h3.about-me__third-title=data['about-me'].thirdTitle
.column(span="row")
    .description-box__with-shadow 
        each description in data['about-me'].descriptionList
            p.description=description
            
.column(span="row" height="10")
.column(span="row")
    button.button__push--me 
        i.icon__download 
        span.inside-button=data['about-me'].downloadButtonLabel
.column(span="row" height="10")
.column(span="row")
    button.button__disc--green 
      i.icon__arrow-down 
.column(span="row" height="10")
