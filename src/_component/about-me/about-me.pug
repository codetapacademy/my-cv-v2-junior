-var sectionTitle= data['about-me'].sectionTitle

include ../section-title/section-title.pug
.column(span="row")        
    h1.about-me__main-title= data['about-me'].mainTitle
.column(span="row")
    h2.about-me__sub-title= data['about-me'].subTitle
.column(span="row" height="10")
.column(span="row")
    h3.about-me__lead-title= data['about-me'].leadTitle
.column(span="row")
    .about-box 
        p.description= data['about-me'].descriptionList
.column(span="row")
.column(span="row" height="10")
.column(span="row" height="10")
    button.button__pill-blue 
        i.icon__download 
        span.button-text= data['about-me'].downloadButtonLabel

.column(span="row")
    button.disc-green     
        i.icon__arrow-down 
        span.button-text 
.column(span="row" height="10")

